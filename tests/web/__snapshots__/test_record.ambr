              $button.text('Export to spreadsheet');
              $button.insertAfter($table);
  
              $button.click(function() {
                  const csv = $table.table2CSV();
                  window.location.href = 'data:text/csv;charset=UTF-8,' + encodeURIComponent(csv);
              });
          });
      </script>
  </body>
  
